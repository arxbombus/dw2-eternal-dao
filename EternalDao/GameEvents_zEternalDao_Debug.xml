    <!-- DEBUG: Eternal Celestials Interval GameEvent -->
    <GameEvent>
        <Name>DEBUG: Eternal Celestials Interval GameEvent</Name>
        <TriggerType>Undefined</TriggerType>
        <TriggerPriority>0</TriggerPriority>
        <AllConditionsMustBeMet>true</AllConditionsMustBeMet>
        <TriggerActionsAreChoices>false</TriggerActionsAreChoices>
        <SuppressTriggerDescriptions>true</SuppressTriggerDescriptions>
        <ConditionEvaluation>Evak</ConditionEvaluation>
        <Conditions>
            <GameEventCondition>
                <Type>CheckColonyEventIntervalSinceLastActiveInEmpire</Type>
                <!-- ColonyEventId, Current Value = We Are Great! -->
                <LookupValue>32500</LookupValue>
                <ComparisonType>GreaterThan</ComparisonType>
                <!-- Time Since (Years) -->
                <ComparisonValue>1</ComparisonValue>
            </GameEventCondition>
        </Conditions>
        <TriggerActions>
            <GameEventAction>
                <Type>GeneralStoryMessageToEmpire</Type>
                <MessageTitle>DEBUG: Eternal Celestials Interval Event Triggered</MessageTitle>
                <Description>This is a debug event for the Eternal Dao government. It triggers at specified intervals.</Description>
                <ActionLocationHint>HomeColony</ActionLocationHint>
                <AllConditionsMustBeMet>true</AllConditionsMustBeMet>
            </GameEventAction>
            <GameEventAction>
                <Type>StartColonyEvent</Type>
                <ActionLocationHint>HomeColony</ActionLocationHint>
                <!-- Colony Event Id, Current Value = DEBUG: Eternal Celestials Interval ColonyEvent -->
                <Id1>32500</Id1>
                <!-- 0=Send Message -->
                <Id2>-1</Id2>
                <!-- If no valid ActionLocation then -1=Empire Capital, 0=Highest Developed Colony, 1=Random Colony -->
                <Id3>-1</Id3>
                <!-- 1=Restart with New Duration Period -->
                <Value1>1</Value1>
                <AllConditionsMustBeMet>true</AllConditionsMustBeMet>
            </GameEventAction>
        </TriggerActions>
    </GameEvent>
